<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.doan.hcpharma.model.KhachHangEntity" table="khachhang" schema="qlnhathuoc">
        <id name="maKh">
            <column name="maKH" sql-type="varchar(255)"/>
        </id>
        <property name="tenKh">
            <column name="tenKH" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="gioiTinh">
            <column name="gioiTinh" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="ngaySinh">
            <column name="ngaySinh" sql-type="date" not-null="true"/>
        </property>
        <property name="sdtKh">
            <column name="sdtKH" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>