<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.doan.hcpharma.model.HoaDonEntity" table="hoadon" schema="qlnhathuoc">
        <id name="maHoaDon">
            <column name="maHoaDon" sql-type="varchar(255)"/>
        </id>
        <property name="ngayLapHd">
            <column name="ngayLapHD" sql-type="date" not-null="true"/>
        </property>
        <property name="tongTien">
            <column name="tongTien" sql-type="double" precision="-1"/>
        </property>
        <property name="maKhachHang">
            <column name="maKhachHang" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="maNhanVien">
            <column name="maNhanVien" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>